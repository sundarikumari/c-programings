#include <stdio.h>

int main() {
      char s[1000];  
    int i;

    printf("Enter the string: ");
    gets(s);

    printf("string in lowercase ='%s'\n",s);


    for(i=0;s[i];i++)  
    {
        if(s[i]=='a')
         s[i]-=32;
 	}


    printf("string in uppercase ='%s'\n",s);

    return 0;
}
