#include <stdio.h>

int main() {
    char str[200];
    int i=0, count=0;
    printf("\n Enter the sentence: ");
    gets(str);
    while(str[i]!='\0')
    {
        if(str[i]==' ' && str[i+1] != ' ')
        count++;
        i++;
    }
    printf("\n The total count of words is: %d",count+1);
    return 0;
}
